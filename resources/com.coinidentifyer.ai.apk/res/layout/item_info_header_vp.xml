<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/iv_image"
        android:background="@drawable/selector_ring_theme_4_ring_white_4"
        android:padding="@dimen/x4"
        android:layout_width="@dimen/x368"
        android:layout_height="@dimen/x368"
        android:foreground="@drawable/ripple_border_less"
        android:scaleType="centerCrop"/>
    <TextView
        android:textSize="@dimen/x48"
        android:textStyle="bold"
        android:textColor="@color/White"
        android:gravity="center"
        android:layout_gravity="center"
        android:id="@+id/tv_no_match"
        android:background="@drawable/shape_oval_64000000"
        android:visibility="gone"
        android:layout_width="@dimen/x360"
        android:layout_height="@dimen/x360"
        android:text="@string/text_no_match"/>
</FrameLayout>
