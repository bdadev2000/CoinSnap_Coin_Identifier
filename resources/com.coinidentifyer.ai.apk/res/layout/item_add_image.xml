<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/item_add_image_0"
    android:layout_width="@dimen/x220"
    android:layout_height="@dimen/x200"
    android:layout_marginTop="@dimen/x10"
    android:layout_marginStart="@dimen/x4"
    android:layout_marginEnd="@dimen/x20">
    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv"
        android:layout_width="@dimen/x200"
        android:layout_height="@dimen/x200"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearance="@style/roundedCornerImageStyle12"/>
    <ImageView
        android:id="@+id/iv_close1"
        android:layout_width="@dimen/x44"
        android:layout_height="@dimen/x44"
        android:src="@drawable/icon_delete_photo"
        android:translationZ="1dp"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
