<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:tag="layout/view_series_collection_folder_layout_0"
    android:background="@drawable/bg_series_collection_folder"
    android:layout_width="@dimen/x192"
    android:layout_height="@dimen/x200">
    <GridLayout
        android:padding="@dimen/x20"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/x8"
        android:rowCount="2"
        android:columnCount="2">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_series_collection_folder_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/x4"
            android:src="?attr/collectionMySetsAddIconRes"
            android:layout_column="0"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_series_collection_folder_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/x4"
            android:layout_column="1"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_series_collection_folder_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/x4"
            android:layout_column="0"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_series_collection_folder_4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="@dimen/x4"
            android:layout_column="1"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"/>
    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
