<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/dialog_add_wish_success_0"
    android:visibility="invisible"
    android:layout_width="@dimen/x520"
    android:layout_height="@dimen/x300">
    <LinearLayout
        android:gravity="center"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/animate_ll"
        android:background="@drawable/shape_rect_solid_white_r_12"
        android:layout_width="@dimen/x520"
        android:layout_height="@dimen/x300">
        <ImageView
            android:id="@+id/heart_iv"
            android:layout_width="@dimen/x84"
            android:layout_height="@dimen/x84"
            android:src="@drawable/icon_wished"/>
        <com.glority.widget.GlTextView
            android:textSize="@dimen/x36"
            android:textColor="@color/Black"
            android:gravity="center"
            android:paddingTop="@dimen/x32"
            android:layout_width="@dimen/x520"
            android:layout_height="wrap_content"
            android:text="@string/wishlist_add"
            android:paddingStart="@dimen/x80"
            android:paddingEnd="@dimen/x80"
            app:auto_resize="false"
            app:medium_style="true"/>
    </LinearLayout>
</FrameLayout>
